{
  "title": "RustyGPT SSE Replay",
  "uid": "rustygpt-sse",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Replay Events by Type",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (type) (increase(rustygpt_sse_replay_events_total[5m]))",
          "legendFormat": "{{type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Replay Duration (ms)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(rustygpt_sse_replay_duration_ms_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "rate(rustygpt_sse_replay_duration_ms_sum[5m]) / rate(rustygpt_sse_replay_duration_ms_count[5m])",
          "legendFormat": "avg",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    }
  ]
}
